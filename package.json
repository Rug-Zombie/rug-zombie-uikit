{
  "name": "@rug-zombie-libs/uikit",
  "version": "0.29.3",
  "description": "Set of UI components for RugZombie projects",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/Rug-Zombie/rug-zombie-uikit.git",
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "build": "rm -rf ./dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^17.0.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@types/testing-library__jest-dom": "^5.14.5",
    "jest-styled-components": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-url": "^8.0.1",
    "@types/lodash": "^4.14.191",
    "lodash": "^4.17.21",
    "react-popper": "^2.2.5",
    "react-transition-group": "^4.4.1",
    "styled-system": "^5.1.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
